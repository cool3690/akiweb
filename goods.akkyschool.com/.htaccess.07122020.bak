RewriteEngine On 
RewriteCond %{HTTPS} !on 
RewriteCond %{REQUEST_URI} !^/[0-9]+\..+\.cpaneldcv$ 
RewriteCond %{REQUEST_URI} !^/\.well-known/pki-validation/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$ 
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
RewriteRule ^首頁?$ index.php
RewriteRule ^註冊帳號?$ member_registered.php
RewriteRule ^會員資料?$ member_select.php
RewriteRule ^會員列表?$ admin/member_list.php
RewriteRule ^登入會員?$ login.php
RewriteRule ^登出?$ logout.php

RewriteRule ^商品登錄?$ goods_insert.php
RewriteRule ^商品購物車?$ goods_cart.php
RewriteRule ^商品編輯?$ goods_list.php
RewriteRule ^商品訂單?$ goods_order.php
RewriteRule ^管理訂單?$ goods_admin_order.php
RewriteRule ^追蹤中?$ goods_track.php
RewriteRule ^商品資訊?$ goods_information.php


RewriteRule ^後台管理?$ backstage.php
RewriteRule ^權限管理?$ permission.php